{
    "name": "imper86/php-allegro-api",
    "description": "PHP SDK for Allegro.pl REST API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "imper",
            "email": "me@imper.info"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "lcobucci/jwt": "^4.0",
        "imper86/http-client-builder": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Imper86\\PhpAllegroApi\\": "src"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "php-http/guzzle6-adapter": "^2.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "imper86/vbump": "^1.0",
        "phpstan/phpstan": "^0.12.86"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
